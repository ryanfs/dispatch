<!DOCTYPE html>
<html>
<head>
  <title>Dispatch</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% flash.each do |name, message| %>
    <%= content_tag(:div, message, class: "alert alert-#{name}") %>
  <% end %>
<%= yield %>

</body>
</html>
